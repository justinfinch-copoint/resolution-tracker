# Database (local devcontainer)
DATABASE_URL=postgresql://postgres:postgres@db:5432/ResolutionTracker

# Supabase (get from https://supabase.com/dashboard > Settings > API)
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-anon-or-publishable-key

# Claude API (get from https://console.anthropic.com)
ANTHROPIC_API_KEY=your-api-key
# Optional: Override default model (default: claude-sonnet-4-20250514)
# ANTHROPIC_MODEL=claude-sonnet-4-20250514

# Default page for authenticated users (server-only)
DEFAULT_PROTECTED_PAGE=/chat
